{
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "google",
    "rules": {
        "new-cap": 0, // Turn off new-cap because Express (e.g. express.Router)
        "max-len": "off",
    },
    "parserOptions": {
        "sourceType": "module"
    }
}